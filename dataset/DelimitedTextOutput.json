{
	"name": "DelimitedTextOutput",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorageCSVFormat",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"fileName": {
				"type": "string"
			},
			"dirName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().fileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().dirName",
					"type": "Expression"
				},
				"container": "converted-table-to-csv-files"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}